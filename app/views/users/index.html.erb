<%= render "filter_form", user: @user %>
<% @users.each do |user| %>
  <%= user.first_name %><br/>
  <%= user.last_name %><br/>
  <%= user.email %><br/>
  <%= user.gender %><br/>
  <%= user.age %><br/>
  <%= user.sport %><br/>
  <%= user.location %><br/>
  <%= link_to "Details", user_path(user) %>
<% end %>
