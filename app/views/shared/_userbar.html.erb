<div class="db-top">
  <div class="self-img">
    <%= cl_image_tag user.photo.key, class: "avatar avatar-bordered" %>
  </div>
  <div class="user-id">
    <div class="gymally-semibold">
      <%= user.first_name.capitalize %>
    </div>
    <div class="gymally-small s-to-p">
      <%= my_gym.name %>
    </div>
  </div>
  <div class="profile-link">
    <%= link_to user_path(user) do %>
      <%= image_tag "settings.svg" %>
    <% end %>
  </div>
</div>
